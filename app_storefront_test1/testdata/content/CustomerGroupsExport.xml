<?xml version="1.0" encoding="UTF-8"?>
<customer-groups xmlns="http://www.demandware.com/xml/impex/customergroup/2007-06-30">
    <customer-group group-id="EyeColorAndSkinType">
        <membership-rule>
            <included-customers>
                <condition-group match-mode="all">
                    <condition>
                        <attribute-path>customer.profile.custom.eyecolor</attribute-path>
                        <operator>exists</operator>
                    </condition>
                    <condition>
                        <attribute-path>customer.profile.custom.skintype</attribute-path>
                        <operator>exists</operator>
                    </condition>
                </condition-group>
            </included-customers>
        </membership-rule>
        <custom-attributes/>
    </customer-group>

